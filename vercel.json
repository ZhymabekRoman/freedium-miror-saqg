{
   "version": 2,
   "builds": [
      {
         "src": "proxy.js",
         "use": "@vercel/node"
      },
      {
         "src": "public/**",
         "use": "@vercel/static"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "proxy.js"
      }
   ],
   "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
